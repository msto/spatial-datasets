
#include: '01_dataset-cleaning/thrane.snake'

subworkflow thrane:
    snakefile: "01_dataset-cleaning/thrane.snake"
    workdir: ".."
    configfile: "snakefiles/01_dataset-cleaning/thrane.yaml"

rule all:
